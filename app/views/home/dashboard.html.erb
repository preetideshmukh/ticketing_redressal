<% if user_signed_in? %>
  <html>
    <head>
      <script src="http://code.highcharts.com/highcharts.js"></script>
      <script src="http://code.highcharts.com/modules/exporting.js"></script>
    </head>
    <body>
      <div class="col-md-2 col-md-offset-1 bg-warning">
        <font size="4">Resolved Ticket:</font>
        <font size="4">
          <%= @ticket_resolved %>
        </font>
      </div>
      <div class="col-md-2 bg-danger">
        <font size="4">Pending Ticket:</font>
        <font size="4">
          <%= @ticket_processing %>
        </font>
      </div>
      <div class="col-md-2 bg-primary">
        <font size="4">New Ticket:</font>
        <font size="4">
          <%= @ticket_new %>
        </font>
      </div>
      <div id="barchart" style="width: 400px; height: 500px; margin-right: 425px; margin-top: 75px">
        <div class="hidden-fields" style="display: none;">
          <%= content_tag "div", id: "BarDiv", data: {stuff: @status} do %>
            <br/>
          <% end %>
        </div>
      </div>
    </body>
  </html>
<% end %>
    


<% @tweet_news.each do |tweet| %>
<ul>
    <li><%= twitter_text(tweet.text) %></li>
</ul>
<% end %>