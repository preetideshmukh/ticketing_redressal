<% @page_title = "New Complaint" %>
<div class="container">
        <div class="row centered-form">
          <div class="col-md-8 col-md-offset-2">  
        
          <div class="panel panel-default">
            <div class="panel-heading">
            <h1>Add New Complaint </h1>
        </div>
        <div class="panel-body">

<html>
  <head>
  </head>
  <body>
        <%= form_for(@ticket,:html=>{:id=>'ticket_form1'}) do|f| %>
          <table>
            <%= render(:partial => "form",:locals=> {:f =>f}) %>
          </table>
          <br/>
          <%= f.submit "Create Request", :class => "btn btn-primary",:style=>"margin-left: 6%" %>
          <%= link_to "Cancel Request", tickets_path, :class => "btn btn-danger" %>
        <% end %>
      </div>
    </div>
  </body>
</html>
</div>
</div>
</div>
</div>
</div